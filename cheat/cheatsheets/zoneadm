# Halt zone
zoneadm -z <zone_name> halt

# Delete Zone 
zoneadm -z <zone_name> halt
zoneadm -z <zone_name> uninstall

# Verify Zone
zoneadm -z <zone_name> verify

# Installing Zone 
zoneadm -z <zone_name> install

# Boot Zone
zoneadm -z <zone_name> boot

# Reboot Zone
zoneadm -z <zone_name> reboot

# List Zones
zoneadm list -cv
